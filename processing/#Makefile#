datatypes = mnist_load.o

#ARMA_INCLUDE_FLAG = -I ../include
LIB_FLAGS = -larmadillo
OPT = -O2
CXX = g++

CXXFLAGS = $(OPT)

all: mnist_load

mnist_load : $(mnist)
	$(CXX) $(CXXFLAGS) -o $@ $^ $(LIB_FLAGS)

#mnist_load : $(mnist)
#	$(CXX) -o $@ $^

clean:
	$(RM) *.o
	$(RM) .depend

depend:
	$(CXX) -MM $(CXXFLAGS) *.cc > .depend

-include .depend